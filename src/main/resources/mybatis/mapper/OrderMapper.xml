<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huang.Dao.OrderMapper">
    <select id="searchAll" resultMap="result">
        select * from order_info;
    </select>
    <select id="relateinfo" resultType="com.huang.pojo.returnList.relatedOrderInfo">
        select * from related_order_info where sku_order_id =#{shop_order_id}
    </select>
    <resultMap id="result" type="com.huang.pojo.returnList.OrderInfo">
        <association property="related_order_info"
                     column="related_order_info"
                     javaType="relatedOrderInfo"
                     select="relateinfo">
        </association>
    </resultMap>

</mapper>